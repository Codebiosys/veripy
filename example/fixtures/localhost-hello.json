{
  "url": "http://www/",
  "elements": {
    "Page Header": {
      "by": "tag",
      "selector": "h1"
    },
    "Hidden Content": {
      "by": "id",
      "selector": "invisible"
    },
    "Required Field": {
      "by": "name",
      "selector": "required-field"
    },
    "Optional Field": {
      "by": "name",
      "selector": "optional-field"
    },
    "Enabled Field": {
      "by": "name",
      "selector": "enabled-field"
    },
    "Disabled Field": {
      "by": "name",
      "selector": "disabled-field"
    },
    "Default Field": {
      "by": "name",
      "selector": "default-field"
    },
    "Text Field": {
      "by": "name",
      "selector": "text-field"
    },
    "Textarea Field": {
      "by": "name",
      "selector": "textarea-field"
    },
    "Number Field": {
      "by": "name",
      "selector": "number-field"
    },
    "Checkboxes": {
      "by": "css",
      "selector": "#checkboxes input",
      "kwargs": {
        "allow_multiple": true
      }
    },
    "Checkbox One": {
      "by": "name",
      "selector": "checkbox-one"
    },
    "Checkbox Two": {
      "by": "name",
      "selector": "checkbox-two"
    },
    "Checkbox Three": {
      "by": "name",
      "selector": "checkbox-three"
    },
    "File Upload Field": {
      "by": "name",
      "selector": "file-field"
    },
    "Select Field": {
      "by": "name",
      "selector": "select-field"
    },
    "Missing Field": {
      "by": "id",
      "selector": "a-field-that-is-not-on-the-page"
    },
    "Random Number": {
      "by": "id",
      "selector": "rando"
    },
    "Other Page Link": {
      "by": "id",
      "selector": "other-page-link"
    },
    "form": {
      "by": "css",
      "selector": "form label",
      "kwargs": {
        "allow_multiple": true
      }
    },
    "Missing Link": {
      "by": "id",
      "selector": "an-element-that-is-not-on-the-page"
    }
  }
}
